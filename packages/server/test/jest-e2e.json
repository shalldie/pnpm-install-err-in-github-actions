{
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
        "@app/model/(.*)": "<rootDir>/../libs/model/src/$1",
        "@app/model": "<rootDir>/../libs/model/src",
        "@app/utils/(.*)": "<rootDir>/../libs/utils/src/$1",
        "@app/utils": "<rootDir>/../libs/utils/src",
        "@app/log/(.*)": "<rootDir>/../libs/log/src/$1",
        "@app/log": "<rootDir>/../libs/log/src",
        "@app/article/(.*)": "<rootDir>/../libs/article/src/$1",
        "@app/article": "<rootDir>/../libs/article/src"
    }
}
